{
  "name": "infquery",
  "version": "0.0.2",
  "description": "a javascript package like Inquirerjs on vscode",
  "files": ["dist"],
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c build/config.js && tsc",
    "watch": "rollup -c build/config.js -w",
    "type": "tsc",
    "type:watch": "tsc -w",
    "dev": "npm-run-all --parallel watch type:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jijiaxin1808/Inquirerjs-for-vscode.git"
  },
  "keywords": [
    "vscode",
    "Inquirerjs"
  ],
  "author": "jijiaxin1808",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jijiaxin1808/Inquirerjs-for-vscode/issues"
  },
  "homepage": "https://github.com/jijiaxin1808/Inquirerjs-for-vscode#readme",
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.13",
    "@rollup/plugin-commonjs": "^17.1.0",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-external-globals": "^0.6.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.13",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/vscode": "^1.52.0",
    "rollup": "^2.38.4",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {

  }
}
